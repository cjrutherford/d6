    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/main.ts",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "env": {
                "NODE_ENV": "development",
                "DB_HOST": "localhost",
                "DB_PORT": "5432",
                "DB_USERNAME": "d6",
                "DB_PASSWORD": "d6",
                "DB_NAME": "d6",
                "ASSET_PATH": "../assets",
            },
            "console": "integratedTerminal"
        }
    ]

}